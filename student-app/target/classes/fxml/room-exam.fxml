<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="800" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.studentapp.controller.RoomController">
    <top>
        <HBox alignment="CENTER" prefHeight="68.0" prefWidth="800.0" spacing="40.0" BorderPane.alignment="CENTER">
            <children>
                <Label prefHeight="26.0" text="Số phòng thi">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="txtRoom" prefHeight="26.0" prefWidth="207.0" />
                <Button fx:id="btnRoom" mnemonicParsing="false" prefHeight="26.0" prefWidth="80.0" text="Submit" />
            </children>
        </HBox>
    </top>

    <center>
        <Pane prefHeight="494.0" prefWidth="800.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="lblShowRoom" layoutX="14.0" layoutY="12.0" prefHeight="29.0" text="">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>

                <ScrollPane fitToWidth="true" hbarPolicy="NEVER" layoutX="19.0" layoutY="41.0" prefHeight="491.0" prefWidth="762.0">
                    <content>
                        <VBox fx:id="vbox" alignment="TOP_CENTER" spacing="10.0" />
                    </content>
                </ScrollPane>

            </children>
        </Pane>
    </center>
</BorderPane>
